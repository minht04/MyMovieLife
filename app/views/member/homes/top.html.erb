<!--アカウントが削除されたときに表示-->
<p id="error">
    <%= flash[:error] %>
</p>

<div id="container">
  <div id="theTarget" class="swiper-container">
    <div class="swiper-wrapper">
      <div class="swiper-slide" width="100%"><%= image_tag 'image1', style:"width: 100%;height: 600px;"%></div>
      <div class="swiper-slide" width="100%"><%= image_tag 'image2', style:"width: 100%;height: 600px;"%></div>
      <div class="swiper-slide" width="100%"><%= image_tag 'image3', style:"width: 100%;height: 600px;"%></div>
      <div class="swiper-slide" width="100%"><%= image_tag 'image4', style:"width: 100%;height: 600px;"%></div>
      <div class="swiper-slide" width="100%"><%= image_tag 'image5', style:"width: 100%;height: 600px;"%></div>
    </div>
    <div class="swiper-button-next"></div>
    <div class="swiper-button-prev"></div>
  </div>
</div>